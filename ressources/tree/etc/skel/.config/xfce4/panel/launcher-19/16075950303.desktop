[Desktop Entry]
Version=1.0
Type=Application
Exec=exo-open --launch WebBrowser %u "http://127.0.0.1"
Icon=/www/favicon.ico
StartupNotify=true
Terminal=false
Categories=Network;X-XFCE;X-Xfce-Toplevel;
OnlyShowIn=XFCE;
X-XFCE-MimeType=x-scheme-handler/http;x-scheme-handler/https;
X-AppStream-Ignore=True
Name=Web Browser
Name[fr]=localhost
Comment=Browse the web
Comment[fr]=localhost
X-XFCE-Source=file:///usr/share/applications/exo-web-browser.desktop
Path=
